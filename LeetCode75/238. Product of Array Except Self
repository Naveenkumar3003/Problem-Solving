class Solution {
public:
    vector<int> productExceptSelf(vector<int>& nums) {
        vector<int> ans(nums.size());
        //left to right
        ans[0]=1;
        for(int i=1;i<nums.size();i++){
            ans[i]=ans[i-1]*nums[i-1];
        }
        //right to left
        int rightProduct=1;
        for(int i=nums.size()-1;i>=0;i--){
            ans[i]=ans[i]*rightProduct;
            rightProduct*=nums[i];
        }
        return ans;
    }
};
